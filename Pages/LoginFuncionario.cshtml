@page
@model Eco_life.Pages.Pages_LoginFuncionario
@{
    ViewData["Title"] = "Login de Funcionário";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Login de Funcionário</title>
    <link rel="stylesheet" href="/css/site.css" />
</head>
<body>
    <h1>Login de Funcionário</h1>
    <form method="post">
        <label for="email">Email:</label>
        <input type="email" name="Email_Funcionario" required /><br />

        <label for="senha">Senha:</label>
        <input type="password" name="Senha_Funcionario" required /><br />

        <button type="submit">Entrar</button>
    </form>
</body>
</html>
